{
	"name": "launcher-front-end",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check 'src/**/*.{js,ts,svelte,json}' && eslint 'src/**/*.{js,ts,svelte}' --fix",
		"format": "prettier --write 'src/**/*.{js,ts,svelte,json}'"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "2.6.0",
		"@skeletonlabs/tw-plugin": "0.3.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.7",
		"@tailwindcss/forms": "^0.5.7",
		"@tanstack/svelte-query": "^5.13.2",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"autoprefixer": "10.4.16",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "8.4.32",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.8",
		"svelte-check": "^3.4.3",
		"tailwindcss": "3.3.6",
		"tslib": "^2.4.1",
		"vite-plugin-tailwind-purgecss": "0.1.4"
	},
	"type": "module",
	"dependencies": {
		"trpc-svelte-query-adapter": "^2.2.0"
	}
}
