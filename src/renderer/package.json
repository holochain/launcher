{
	"name": "launcher-front-end",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check 'src/**/*.{js,ts,svelte,json,postcss}' && eslint 'src/**/*.{js,ts,svelte}' --fix",
		"format": "prettier --write 'src/**/*.{js,ts,svelte,json,postcss}'"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "2.9.0",
		"@skeletonlabs/tw-plugin": "0.3.1",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tanstack/svelte-query": "^5.28.9",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"autoprefixer": "10.4.19",
		"clsx": "^2.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"i18next": "^23.10.0",
		"postcss": "8.4.38",
		"prettier-plugin-sort-json": "^4.0.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"svelte": "^4.2.12",
		"svelte-check": "^3.4.3",
		"svelte-i18next": "^2.2.2",
		"tailwindcss": "3.4.3",
		"trpc-svelte-query-adapter": "^2.2.4",
		"vite-plugin-tailwind-purgecss": "0.2.1"
	},
	"type": "module"
}
