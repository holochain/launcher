{
	"name": "launcher-front-end",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check 'src/**/*.{js,ts,svelte,json,postcss}' && eslint 'src/**/*.{js,ts,svelte}' --fix",
		"format": "prettier --write 'src/**/*.{js,ts,svelte,json,postcss}'"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "2.8.0",
		"@skeletonlabs/tw-plugin": "0.3.1",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tanstack/svelte-query": "^5.21.7",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"autoprefixer": "10.4.17",
		"clsx": "^2.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"i18next": "^23.9.0",
		"postcss": "8.4.35",
		"prettier-plugin-sort-json": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.8",
		"svelte-check": "^3.4.3",
		"svelte-i18next": "^2.2.2",
		"tailwindcss": "3.4.1",
		"trpc-svelte-query-adapter": "^2.2.3",
		"tslib": "^2.4.1",
		"vite-plugin-tailwind-purgecss": "0.2.0"
	},
	"type": "module"
}
